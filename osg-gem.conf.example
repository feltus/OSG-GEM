input1 = input1 = ./Test_data/SRR4343300.fastq.gz

[config]

# process using TopHat2
tophat2 = False

# process using Hisat2
hisat2 = True

# process using Cufflinks
cufflinks = False

# process using StringTie
stringtie = True

